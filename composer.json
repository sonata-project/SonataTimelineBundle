{
    "name": "sonata-project/timeline-bundle",
    "type": "symfony-bundle",
    "description": "Integrates SpyTimelineBundle into Sonata",
    "keywords": [
        "sonata",
        "date",
        "time",
        "wall"
    ],
    "homepage": "https://sonata-project.org/bundles/timeline",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataTimelineBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/orm": "^2.2",
        "sonata-project/block-bundle": "^3.11",
        "sonata-project/core-bundle": "^3.9",
        "sonata-project/easy-extends-bundle": "^2.5",
        "sonata-project/intl-bundle": "^2.4",
        "stephpy/timeline-bundle": "^2.3 || ^3.0",
        "symfony/config": "^3.4 || ^4.2",
        "symfony/dependency-injection": "^3.4 || ^4.2",
        "symfony/doctrine-bridge": "^3.4 || ^4.2",
        "symfony/form": "^3.4 || ^4.2",
        "symfony/framework-bundle": "^3.4 || ^4.2",
        "symfony/http-foundation": "^3.4 || ^4.2",
        "symfony/http-kernel": "^3.4 || ^4.2",
        "symfony/options-resolver": "^3.4 || ^4.2",
        "symfony/security-core": "^3.4 || ^4.2",
        "twig/twig": "^1.35 || ^2.4"
    },
    "conflict": {
        "sonata-project/admin-bundle": "<3.31 || >=4.0",
        "sonata-project/user-bundle": "<4.3"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.1",
        "sonata-project/admin-bundle": "^3.31",
        "sonata-project/user-bundle": "^4.3",
        "symfony/phpunit-bridge": "^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\TimelineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\TimelineBundle\\Tests\\": "tests/"
        }
    }
}
